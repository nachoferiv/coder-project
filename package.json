{
  "name": "coder-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cleanTS": "rimraf dist/",
    "cleanBabel": "rimraf distBabel/",
    "copy-filesTS": "copyfiles -u 1 src/**/*.html src/**/*.css src/**/*.jpg src/**/*.hbs src/**/*.json dist/",
    "copy-filesBabel": "copyfiles -u 1 src/**/*.html src/**/*.css src/**/*.jpg src/**/*.hbs src/**/*.json distBabel/",
    "build": "yarn cleanTS && .\\node_modules\\.bin\\tsc && yarn copy-filesTS",
    "ServerTStoJS5": "yarn cleanTS && .\\node_modules\\.bin\\tsc && yarn copy-filesTS",
    "ServerJStoJS5": "yarn cleanBabel && babel src --out-dir distBabel && yarn copy-filesBabel",
    "devTS": "nodemon ./dist/index.js",
    "devBabel": "nodemon ./distBabel/index.js",
    "watch": ".\\node_modules\\.bin\\tsc -w",
    "start": "nodemon ./dist/index.js",
    "dev": "nodemon ./dist/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "express-handlebars": "^5.2.1",
    "node-fetch": "^2.6.1",
    "socket.io": "^4.0.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.37",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "copyfiles": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
